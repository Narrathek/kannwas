{
  "name": "kannwas",
  "type": "module",
  "version": "1.0.0",
  "description": "simple helper library to create 2D canvas games",
  "main": "src/kannwas.mjs",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "postinstall": "node ./node_modules/frontend-dependencies/index.js",
    "lint": "standard \"src/*.mjs\" \"src/util/*.mjs\" \"src/math/*.mjs\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Narrathek/kannwas.git"
  },
  "frontendDependencies": {
    "target": "src/vendor",
    "packages": {
      "arcsecond": {
        "url": "https://github.com/francisrstokes/arcsecond"
      }
    }
  },
  "keywords": [
    "canvas",
    "game",
    "javascript",
    "kannwas"
  ],
  "author": "Simon Petereit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Narrathek/kannwas/issues"
  },
  "homepage": "https://github.com/Narrathek/kannwas#readme",
  "dependencies": {
    "frontend-dependencies": "^1.1.8"
  },
  "devDependencies": {
    "jest": "^26.4.2",
    "standard": "^14.3.4"
  }
}
